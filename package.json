{
  "name": "express-bp",
  "version": "1.2.1",
  "description": "Express CLI Tool for creating Boiler plate, automatic DB intializing and module creating",
  "type": "module",
  "files": [
    "bin",
    "dist",
    "!.gitattributes"
  ],
  "preferGlobal": true,
  "scripts": {
    "test": "node ./dist/index.js",
    "dev": "tsc",
    "unlink": "npm uninstall -g ",
    "link": "npm i -g",
    "start": "npm run clean && npm run build && npm run unlink && npm run link && clear",
    "startW": "npm run cleanWindows && npm run build && npm run unlink && npm run link",
    "clean": "rm -rf dist",
    "cleanWindows": "rmdir /s dist",
    "build": "npm run dev && node scripts/copyTemplates.js",
    "pub": "npm publish --production"
  },
  "main": "dist/index.js",
  "bin": {
    "express-bp": "./bin/index.js"
  },
  "keywords": [
    "express",
    "expressautomactic",
    "expresscomponent",
    "expresscli",
    "cli",
    "component",
    "expressgenerator",
    "generator",
    "backend",
    "backendnode",
    "server",
    "mongodb",
    "dynamodb",
    "autoexpress",
    "express-js",
    "expressjs",
    "expressts",
    "modulegenerator",
    "simple-express",
    "expresssimple"
  ],
  "engines": {
    "node": ">=16.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nevinedwin/express-cli-tool.git"
  },
  "author": "Nevin Edwin",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.4",
    "@types/node": "^20.8.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.11",
    "ora": "^7.0.1"
  }
}